#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int a[3],b[3];
	    for(int i=0;i<3;i++){
	        cin>>a[i];
	    }
	    for(int i=0;i<3;i++){
	        cin>>b[i];
	    }
	    int count1=0,count2=0;
	    for(int i=0;i<3;i++){
	        if(a[i]>b[i]){
	            count1++;
	        }
	        else if(a[i]<b[i]){
	            count2++;
	        }
	    }
	    if(count1>count2) cout<<"A\n";
	    else if(count2>count1) cout<<"B\n";
	}
	return 0;
}
