#include <iostream>
using namespace std;

int main() {
	    int n,h,x;
	    cin>>n>>h>>x;
	    int a[n];
	    bool check=false;
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    for(int i=0;i<n;i++){
	        if(x+a[i]==h){
	            check=true;
	            break;
	        }
	    }
	    if(check){
	        cout<<"YES"<<endl;
	    }
	    else
	        cout<<"NO"<<endl;

	return 0;
}
